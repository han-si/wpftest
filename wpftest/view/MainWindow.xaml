        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:viewmodel="clr-namespace:wpftest.viewmodel" xmlns:view="clr-namespace:wpftest.view" x:Class="wpftest.MainWindow"
        Title="MainWindow" Height="350" Width="525">
	<Grid Margin="0,0,2,1">
		<Grid.DataContext>
			<viewmodel:MainViewModel/>
		</Grid.DataContext>
		<TabControl HorizontalAlignment="Left" Height="232" Margin="10,10,0,0" VerticalAlignment="Top" Width="460">
			<TabItem Header="TabItem">
				<Grid Background="#FFE5E5E5" Margin="0,0,-33,-49">
					<view:coyote HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" Height="243" Width="467"/>
				</Grid>
			</TabItem>
			<TabItem Header="TabItem">
				<Grid Background="#FFE5E5E5"/>
			</TabItem>
		</TabControl>
	</Grid>
</Window>
