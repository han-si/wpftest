<UserControl
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:viewmodel="clr-namespace:wpftest.viewmodel" x:Class="wpftest.view.coyote" 
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">
	<Grid>
		<Grid.DataContext>
			<viewmodel:coyoteviewmodel/>
		</Grid.DataContext>
		<CheckBox Content="CheckBox" HorizontalAlignment="Left" Margin="111,63,0,0" VerticalAlignment="Top" IsChecked="{Binding Checked}"/>
		<Calendar HorizontalAlignment="Left" Margin="37,111,0,0" VerticalAlignment="Top"/>

	</Grid>
</UserControl>
